
<table class="table table-striped">
	  <thead>
	  		<tr>
	  			<th> User Name </th>	
	  			<th> Phone Number </th>	
	  			<th> Date and Time </th>	
	  			<th> Barber </th>	
	  			<th> Color </th>	
	  		</tr>>	
	  </thead>

	  <body>
		<% @results.each do |row| %>
	  		<tr>
	   			<td><%= row['username'] %></td>
	   			<td><%= row['phone'] %></td>
	   			<td><%= row['datestamp'] %></td>
	  			<td><%= row['barber'] %></td>
	  			<td><%= row['color'] %></td>
	  		</tr>

		<% end %>
	  </body>
</table>



